@model MyFace.Models.View.PostsViewModel

@{
    Layout = "_Layout";
}
<a class="nav-link text-dark" asp-area="" asp-controller="Posts" asp-action="CreatePostPage">Create a Posts</a>

<ul>
    @foreach (var post in Model.Posts)
    {
        <li>@post.Message</li>

        <li>@post.PostedAt</li>
        <li>@post.PostedBy.DisplayName</li>
            <img src="@post.ImageUrl" alt="Post Image"/>   
    }
</ul>